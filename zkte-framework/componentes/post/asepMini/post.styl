
/**********Emprendedor**************/
.Emprendedor-Img,.Emprendedor-Contenido,.Emprendedor-RedesSociales
	display inline-block
	vertical-align middle
	margin-left -4px
	@media screen and (max-width: 663px)
		margin-left 0px
.Emprendedor
	border-bottom bordePost
	padding-top 1em
	padding-bottom 1em
	color colorLetra
	transition all ease 0.3s
	-ms-transition all ease 0.3s
	@media screen and (max-width: 663px)
		//flex-direction column
	&:hover
		background backgroundHover
	&-Nombre
		font-weight bold
		color colorLetraTitulo
		@media screen and (max-width: 663px)
			text-align center
	&-Cargo
		@media screen and (max-width: 663px)
			text-align center
	&-Img
		width 20%
		text-align center		
		text-decoration none	
		@media screen and (max-width: 663px)
			width 100%
			//padding-left 5%
			//padding-right 6%
		& img
			height 8em
			border-radius 50%
			border none
	&-Contenido
		width 65%
		text-decoration none
		color colorLetra
		@media screen and (max-width: 663px)
			width 100%
			text-align center
			//padding-left 5%
			//padding-right 6%
	&-RedesSociales
		width 15%
		@media screen and (max-width: 663px)
			width 100%
			//padding-left 5%
			//padding-right 6%
		& i
			font-size tamanioRedesSociales
			margin-left marginRedesSociales
			color colorRedesSociales
.estrellitas
	margin-bottom 1em
.estrellitas,.redes
	text-align center
.fa-star
	font-size 1em !important
.starActivada
	color colorEstrella !important
/**********Emprendedor**************/