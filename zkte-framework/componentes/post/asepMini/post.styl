plomo1=#6b6767
colorLetraTitulo=#2ca7df
colorEstrella=#2ca7df
/**********Emprendedor**************/
.Emprendedor-Img,.Emprendedor-Contenido,.Emprendedor-RedesSociales
	display inline-block
	vertical-align middle
	margin-left -4px
.Emprendedor
	border-bottom 1px solid #B1ADAD
	padding-top 1em
	padding-bottom 1em
	color plomo1
	@media screen and (max-width: 663px)
		//flex-direction column
	&-Nombre
		font-weight bold
		color colorLetraTitulo
		@media screen and (max-width: 663px)
			text-align center
	&-Cargo
		@media screen and (max-width: 663px)
			text-align center
	&-Img
		width 20%
		text-align center	
		@media screen and (max-width: 663px)
			width 100%
			padding-left 5%
			padding-right 6%
		& img
			height 8em
			border-radius 50%
	&-Contenido
		width 65%
		@media screen and (max-width: 663px)
			width 100%
			padding-left 5%
			padding-right 6%
	&-RedesSociales
		width 15%
		@media screen and (max-width: 663px)
			width 100%
			padding-left 5%
			padding-right 6%
		& i
			font-size 2.5em
			margin-left 0.5em
			color plomo1
.estrellitas
	margin-bottom 1em
.estrellitas,.redes
	text-align center
.fa-star
	font-size 1em !important
.starActivada
	color colorEstrella !important
/**********Emprendedor**************/