.desactivarTab
	background #b4b4b4 !important// backgroundPestania
.activarTab
	background #DEE0E2 !important//hoverPestaniaBackground
.TabsAsep
	display flex
	justify-content flex-start
	@media screen and (max-width: 750px)
		display none
	& a
		text-decoration none
	&-Pestania
		height inherit
		background #b4b4b4// backgroundPestania
		padding 0.6em 0.5em
		cursor pointer
		border-right 1px solid #8A8A8A //bordePestania
		transition all ease 0.3s
		&:hover
			background #DEE0E2 //hoverPestaniaBackground
		&-Bloque2
			margin-top 0.4em
		&-Ttulo
			display flex
			color #5D5C5C //backgroundNumero
			&-Numero
				background #5D5C5C //backgroundNumero
				border-radius 50%
				height 1.9em
				width 3em
				color white //colorNumero
				display flex
				justify-content center
				align-items center
				margin-right 0.5em
		&-Contenido
			color #777777 //colorContenido
			margin-top 0.4em
.TabsAsepMovil
	display none
	@media screen and (max-width: 750px)
		display block
	& a
		text-decoration none
	&-Pestania
		background #b4b4b4// backgroundPestania
		transition all ease 0.3s
		text-align center
		border-bottom 1px solid #8A8A8A //bordePestania
		display flex
		justify-content center
		align-items center
		cursor pointer
		padding 0.5em 0em
		&:hover
			background #DEE0E2 //hoverPestaniaBackground		
		&-Numero
			color white //colorNumero
			background #777777 //colorContenido
			border-radius 50%
			height 2em
			width 2em
			display flex
			justify-content center
			align-items center
			margin-right 1em
		&-Titulo
			color #5D5C5C //backgroundNumero	
			