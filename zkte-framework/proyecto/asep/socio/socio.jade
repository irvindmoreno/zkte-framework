						//email,alfanumerico,numerico,alfabeto,rangoCaracteres
						- idInput="nombreComercialInput"
						- placeholder=""
						- tipo="text"
						- label="Nombre Comercial: (*)"
						include ../../../componentes/input/textAsep/input.jade
						//input-textAsep
					div(class="contenedorDeInputsPaso2")
						//input-textAsep
						//email,alfanumerico,numerico,alfabeto,rangoCaracteres
						- idInput="paginaWebInput"
						- placeholder=""
						- tipo="text"
						- label="Página web"
						include ../../../componentes/input/textAsep/input.jade
						//input-textAsep					
					div(class="contenedorDeInputsPaso2")
						//input-textAsep
						//email,alfanumerico,numerico,alfabeto,rangoCaracteres
						- idInput="facebookInput"
						- placeholder=""
						- tipo="text"
						- label="Facebook: "
						include ../../../componentes/input/textAsep/input.jade
						//input-textAsep
					div(class="contenedorDeInputsPaso2")
