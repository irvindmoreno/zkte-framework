rutaComponentes='../../../componentes'
rutaImagenes='../../../imagenes'

.tituloAsep-2
	margin-top 2em
.bloqueIzquierda,.bloqueDerecha,.bloqueCentro
	display inline-block
	vertical-align top
	margin 0
	height inherit
.bloqueIzquierda
	width 23%
	//background red
	min-width 15em
	&-Lupa
		text-align center
		padding-top 1em
		padding-bottom 1em
.DirecotrioInfo-Header-Titulo
	margin-left 2em
.DirecotrioInfo-Texto
	text-align justify
	color #4a4949
	margin-top 2em
.DirecotrioInfo-btnMegusta
	padding 1em 0em
	text-align center
.bloqueCentro
	width 65%
	padding-left 1em
	//background green
	margin-left -5px
	padding-top 2em
.ContenedorPaginacion
	text-align center
	padding 4em 0em
.DirecotrioInfo-Header img,.DirecotrioInfo-Header-Titulo
	display inline-block
	vertical-align bottom
.Emprendedor-Img img
	height 8em
	border-radius 50%
	border none
.Emprendedor-Nombre
	font-weight bold
	color #2ca7df
	@media screen and (max-width: 663px)
		text-align center
.Emprendedor-Cargo
	color #6b6767
	@media screen and (max-width: 663px)
			text-align center
@media screen and (max-width: 1330px)
	.bloqueIzquierda
		width 100%
	.bloqueCentro
		width 100%
		margin-left 0px
		padding-left 0em
	.bloqueIzquierda-Lupa
		text-align center
	.searchAsep-Input
		width 86% !important

