.tituloAsep-2
	margin-top 2em
.bloqueIzquierda,.bloqueDerecha
	display inline-block
	vertical-align top
	margin 0
	height inherit
.bloqueIzquierda
	width 23%
	//background red
	min-width 15em
	&-Lupa
		text-align center
		padding-top 1em
		padding-bottom 1em
.bloqueDerecha
	width 77%
	background green
	margin-left -5px
@media screen and (max-width: 1330px)
	.bloqueIzquierda
		width 100%
	.bloqueDerecha
		width 100%
		margin-left 0px
	.bloqueIzquierda-Lupa
		text-align right
	.searchAsep-Input
		width 92%

