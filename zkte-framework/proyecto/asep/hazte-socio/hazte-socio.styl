rutaComponentes='../../../componentes'
rutaImagenes='../../../imagenes'
.pest-Titulo
	padding 0.2em 2em
	font-size 1.6em
	text-align center
	color #2F2B2B
	@media screen and (max-width: 700px)
		font-size 1.2em
.asterisco
	margin-left 0.3em
.btn-guardarValidado
	text-decoration none
.labels-Cont
	display flex
	color #4E4444
	padding 0.4em 0em
	display flex
	align-items center
	@media screen and (max-width: 680px)
		flex-direction column
		width 100%
		text-align left
		align-items flex-start
@media screen and (max-width: 680px)
	.SelectAsep
		width 100% !important
	.asterisco
		display none
	.labels
		padding 0em 1em
	.radioMultipleMovil
		display flex
		justify-content center
	.textareaPaso2
		width 100% !important
	.buttonAgregarOtraEmpresa input
		margin-left 53.5% !important
		margin-top 1em
.labels
	display flex
	flex-direction column
	align-items center
	margin-top 1em	
.label-texto
	width 12em
	text-align right
	padding-right 1em
	@media screen and (max-width: 680px)
		width 100%
		text-align left
		padding-right 0em
.ContenedorDelTab
	padding-top 3em
	background rgb(222, 224, 226)
	padding-bottom 3em
.label-input
	width 24em
	@media screen and (max-width: 680px)
		width 100%
.radioButonLinea
	display flex
.radioButonLinea input
	margin-left 3em
.label-input-MultiRadio
	flex-start
.UltimoRadio
	padding 2em 9%
	text-align justify
	color #4e4444
	display flex
	& a
		color #2ca7df
.obligatorio
	color #4e4444
	display flex
	justify-content center
.botonGuardar
	display flex
	justify-content center
	margin-top 1em
.datosDeLaEmpresa
	padding-left 10%
	font-size 1.5em
	margin-top 26px
	font-weight bold
	color #353232
.textareaPaso2
	width 26em
	height 11em
	outline none
.buttonAgregarOtraEmpresa input
	font-size 1em
	background #616669
	color #2F2D2D
	margin-left 23%
	margin-top 1em
.buttonAgregarOtraEmpresa input:hover
	color white
.BrowseFile
	display flex
	align-items center
.BrowseFile span
	margin-left 1em
.claseEspecial
	position relative
	top -2em
.inputNumber
	height 2.5em
	outline none
.fechaNacimientoSlects
	display flex
